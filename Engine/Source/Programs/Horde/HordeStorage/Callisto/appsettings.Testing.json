{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ],
    "Enrich": [ "FromLogContext" ]
  },

  "Auth": {
    "Method": "Disabled"
  },

  "Jupiter": {
    "CurrentSite": "Test"
  },

  "Callisto": {
    "TransactionLogRoot": "./callisto-logs",
    "transactionLogImplementation": "file",
    "VerifySerialization": "true"
  },

  "AllowedHosts": "*"
}
